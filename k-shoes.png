    const variantSelect = event.querySelector(".custom_VariantSelect");

    if (variantSelect.classList.contains("show_Variants")) {
      variantSelect.classList.remove("show_Variants");
    } else {
      variantSelect.classList.add("show_Variants");
    }
  } catch (error) {
    console.error("Error in custom_CartVariantFunction function:", error);
  }
}

function cartCustomQtyFunction(event) {
  try {
    document.querySelectorAll(".custom-select").forEach((item) => {
      if (item !== event) {
        item.classList.remove("addDisplay");
      }
    });
    if (event.classList.contains("addDisplay")) {
      event.classList.remove("addDisplay");
    } else {
      event.classList.add("addDisplay");
    }
  } catch (error) {
    console.error("Error in cartCustomQtyFunction function:", error);
  }
}

