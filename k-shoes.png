<script>
function custom_CartVariantFunction(event) {
  try {
    const variantSelect = event.querySelector(".custom_VariantSelect");

    if (variantSelect.classList.contains("show_Variants")) {
      variantSelect.classList.remove("show_Variants");
    } else {
      variantSelect.classList.add("show_Variants");
    }
  } catch (error) {
    console.error("An error occurred in custom_CartVariantFunction:", error);
  }
}



async function addCartVariant(
  cartVariant_Quantity,
  cartVariant_OptionId,
  cartVariant_OptionQtyAvailable,
  event
) {
  let variantQuantity;
  if (cartVariant_OptionQtyAvailable >= cartVariant_Quantity) {
    variantQuantity = cartVariant_Quantity;
  } else {
    variantQuantity = cartVariant_OptionQtyAvailable;
    event
      .closest(".custom-cart-item__media-div2")
      .querySelector(
        ".variantsAvailableText"
      ).innerText = `Only ${variantQuantity} available `;
    event
      .closest(".custom-cart-item__media-div2")
      .querySelector(".variantsAvailableText").style.display = "inline-block";
    setTimeout(() => {
      event
        .closest(".custom-cart-item__media-div2")
        .querySelector(".variantsAvailableText").style.display = "none";
    }, 3000);
  }

  let requestOptions = {
    method: "POST",
    headers: {
      "Content-Type": "application/json",
      "X-Requested-With": "XMLHttpRequest",
    },
    body: JSON.stringify({
      items: [
        {
          id: cartVariant_OptionId,
          quantity: variantQuantity,
        },
      ],
    }),
  };

  try {
    let response = await fetch("/cart/add.js", requestOptions);
    let result = await response.json;
    updateCart();
    return result;
  } catch (err) {
    throw err;
  }
}

async function deleteCartVariant(cartVariant_Id) {
  let variantBody = {
    updates: {
      [cartVariant_Id]: 0,
    },
  };

  let requestOptions = {
    method: "POST",
    headers: {
      "Content-Type": "application/json",
      "X-Requested-With": "XMLHttpRequest",
    },
    body: JSON.stringify(variantBody),
  };

  try {
    let response = await fetch("/cart/update.js", requestOptions);
    let result = await response.json;
    return result;
  } catch (err) {
    throw err;
  }
}


async function updateCartVariant( cartVariant_Quantity,cartVariant_OptionId,cartVariant_OptionQtyAvailable,event) {
  let parentElement = event.closest('.inner_dlfex_qty');
  let loader = parentElement.querySelector('.loading__spinner');
  let variantToDelete = Number(parentElement.getAttribute('current-variant-id'));
  let variantQtyToUpdate = parentElement.querySelector('.select-selected').getAttribute('value');
  let newVAriantCartQty = cartVariant_Quantity;
  let newVAriantInventoryQty = cartVariant_OptionQtyAvailable;  
  let allCartVariantsArray = [];
  let clickedVariantId = Number(event.getAttribute("cartVariant_OptionId"));
  let isVariantAvailable;


  if(variantQtyToUpdate > newVAriantInventoryQty) {
    alert('Quantity Excedding the available Quantity');
  } else {
    
    
    document.querySelectorAll("[data-SelectedVariantId]").forEach((e) => {
      allCartVariantsArray.push(Number(e.getAttribute("data-SelectedVariantId")));
    });
    
    allCartVariantsArray.forEach((e) => {
      if (e == clickedVariantId) {
      isVariantAvailable = true;
    }
  });
  
  if (isVariantAvailable == true) {
    alert('This Variant is already added ');
  } else {

    await deleteCartVariant(variantToDelete);
    await addCartVariant(
      variantQtyToUpdate,
      cartVariant_OptionId,
      cartVariant_OptionQtyAvailable,
      event
    );
    loader.classList.remove('hidden');
  }
}
}
function updateCart() {
  fetch(`${routes.cart_url}?section_id=cart-drawer`)
  .then((response) => response.text())
  .then((responseText) => {
    const html = new DOMParser().parseFromString(responseText, 'text/html');
    document.querySelector('cart-drawer').innerHTML = '';
    document.querySelector('cart-drawer').append(html.documentElement.querySelector('#CartDrawer'));
    const allItems = document.querySelectorAll('.cart-item');
    let sum = 0;
    if(allItems.length > 0){
      allItems.forEach((item)=>{
          let itemCount = Number(item.querySelector('.custom-select-selected').getAttribute('value'));
          sum += itemCount;
      })
      document.querySelector('.cart-count-bubble').innerText = sum;
      document.querySelector('#CartDrawer-Form').style.display = 'block';
      document.querySelector('.drawer__header').style.display = 'flex';
      document.querySelector('.drawer__header').style.width = '100%';
      document.querySelector('.cart__ctas').style.transform = 'translate(0%)';
    } else {
      document.querySelector('#CartDrawer-Form').style.display = 'none';
    }
  });
setTimeout(function() {
    ReloadSmartWishlist();
}, 3000);
}


