    document.addEventListener('DOMContentLoaded', () => {
        if(window.innerWidth < 768) {
            setEventOnSizes();
            window.addEventListener("scroll", getFooterPosition);
        }
    })
    
    function setEventOnSizes() {
        const allSizeVariants = document.querySelectorAll('#MainProduct-template--18680426725604__main .varinat_cs_dflex input');
        console.log("ðŸš€ ~ setEventOnSizes ~ allSizeVariants:", allSizeVariants)
        allSizeVariants.forEach((variant) => {
            variant.addEventListener('click', (event) => {
                sizeClicked(event)
            })
        })
    }
    
    function sizeClicked(event) {
        let variantID = event.target.getAttribute('variantid');
        let mobileForm = document.querySelector('#MainProduct-template--18680426725604__main .duplicate_buys_btns form');
        let variantIdInput = mobileForm.querySelector('.product-variant-id');
        variantIdInput.value =  variantID;
    }
    
    function getFooterPosition() {
        let a = document.querySelector('.footer');
        let footerPosition = a.getBoundingClientRect();
        let windowHeight = window.innerHeight;
        let mobileAtc = document.querySelector('#MainProduct-template--18680426725604__main .duplicate_buys_btns');
        
        if(footerPosition.top <= windowHeight) {
            mobileAtc.style.display = 'none';
        } else {
            mobileAtc.style.display = 'block';
        }
    }
    
</script>


