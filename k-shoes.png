<script>
  window.addEventListener('resize', () => {
    swiperThumbs.zoom = window.innerWidth < 767;
    swiperThumbs.update(); // Update Swiper instance
  });
  document.addEventListener('DOMContentLoaded', function () {
    try {
      initialiseMainSwiper();
      let popup = document.getElementById('popupidmaincs');
      let zoomIcon = document.querySelector('.zoom-icon');
      let closeBtn = document.querySelector('.close');

      let swiperThumbs = new Swiper('.mySwiperThumbs-template--18680426725604__main', {
        spaceBetween: 10,
        slidesPerView: 4,
        freeMode: true,
        watchSlidesProgress: true,
        direction: 'vertical',
      });

      let swiperPopup = new Swiper('.mySwiperPopup-template--18680426725604__main', {
        spaceBetween: 10,
        autoHeight: true,
        navigation: {
          nextEl: '.swiper-button-next',
          prevEl: '.swiper-button-prev',
        },
        thumbs: {
          swiper: swiperThumbs,
        },
      });

      zoomIcon.onclick = function () {
        try {
          if (window.innerWidth > 768) {
            let activeIndex = swiper2.activeIndex;
            swiperPopup.slideTo(activeIndex);
          }
          popup.style.display = 'block';
          document.body.classList.add('no-scroll');
          swiperPopup.update();
        } catch (error) {
          console.error('Error opening popup: ', error);
        }
      };

      closeBtn.onclick = function () {
        try {
          popup.style.display = 'none';
          document.body.classList.remove('no-scroll');
        } catch (error) {
          console.error('Error closing popup: ', error);
        }
      };

      window.onclick = function (event) {
        try {
          if (event.target == popup) {
            popup.style.display = 'none';
            document.body.classList.remove('no-scroll');
          }
        } catch (error) {
          console.error('Error handling window click: ', error);
        }
      };
    } catch (error) {
      console.error('Error initializing Swiper or event listeners: ', error);
    }
  });
</script>


