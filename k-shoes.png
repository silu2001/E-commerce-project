          if (items.length === 0) {
            return;
          }
          items.forEach((item) => {
            let text = item.textContent.trim();
            const hyphenIndex = text.indexOf('-');
            if (hyphenIndex !== -1) {
              // Extract text before and after the hyphen
              let textBeforeHyphen = text.substring(0, hyphenIndex + 1).trim();
              let textAfterHyphen = text.substring(hyphenIndex + 1).trim();
              item.innerHTML = `${textBeforeHyphen} <span style="color: #223C8C;">${textAfterHyphen}</span>`;
