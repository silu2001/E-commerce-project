<script>
 let accordions = document.querySelectorAll(".header .accordions");
 let panels = document.querySelectorAll(".header .panels");
function handleAccordionClick() {
    let isActive = this.classList.contains("active");
    let currentPanel = this.nextElementSibling;
    let iconArrow = this.querySelector('.icon-arrow');

    // Loop through all accordions and close them
    for (let j = 0; j < accordions.length; j++) {
        let panel = accordions[j].nextElementSibling;
        let icon = accordions[j].querySelector('.icon-arrow');
        
        accordions[j].classList.remove("active");
        
        if (panel) {
            panel.style.maxHeight = null; 
        }
        
        if (icon) {
            icon.classList.remove('rotated'); 
        }
    }

    if (!isActive) {
        this.classList.add("active");

        if (currentPanel) {
            currentPanel.style.maxHeight = currentPanel.scrollHeight + "px"; 
        }
        
        if (iconArrow) {
            iconArrow.classList.add('rotated'); 
        }
    }
}


for (let i = 0; i < accordions.length; i++) {
    accordions[i].addEventListener("click", handleAccordionClick);
}


function iconrotation(event){
  try{
  let openingarrow = event.target.closest('details');
  let arrowIcon = openingarrow.querySelector('summary .header-arrow');
  
  if (arrowIcon.style.transform === "rotate(90deg)") {
      arrowIcon.style.transform = "rotate(0deg)";
  } else {
      arrowIcon.style.transform = "rotate(90deg)";
  }
}catch(e){
  console.log(e);
}
}

</script>
